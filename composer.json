{
    "name": "jeremykenedy/laravel-users",
    "description": "Laravel Users Management CRUD Package",
    "keywords": ["laravel", "users", "management", "CRUD"],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "jeremykenedy",
            "email": "jeremykenedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravelcollective/html": "^5.4|^5.5|^5.6|^5.7|^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\laravelusers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jeremykenedy\\laravelusers\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "illuminate/support": "^5.7",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0",
        "laravel/laravel": "5.7.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\laravelusers\\LaravelUsersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
